{
  "name": "gbr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "NEXT_PRIVATE_BUILD_WORKER=webpack turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "colleges:scrape": "pnpm --filter @workspace/scraper debug && pnpm --filter @workspace/scraper parse:save",
    "colleges:import": "pnpm --filter @workspace/scraper import"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "overrides": {
      "csstype": "3.1.3"
    }
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
